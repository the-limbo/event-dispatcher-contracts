{
    "name": "the-limbo/event-dispatcher-contracts",
    "type": "library",
    "description": "Generic abstractions for event dispatcher.",
    "keywords": [
        "the-limbo",
        "contracts",
        "event",
        "dispatcher",
        "psr-14"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "dr0n1k",
            "email": "a.dronov4job@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Limbo\\Contracts\\EventDispatcher\\": "./src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "psr/event-dispatcher": ">=1.0",
        "the-limbo/factory-contracts": "dev-master"
    },
    "provide": {
        "psr/event-dispatcher-implementation": ">=1.0",
        "the-limbo/factory-contracts-implementation": "dev-master"
    },
    "suggest": {
        "the-limbo/event-dispatcher-contracts-implementation": ""
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
